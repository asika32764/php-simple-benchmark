{
    "name": "asika/simple-benchmark",
    "type": "console",
    "description": "PHP Simple Benchmark framework",
    "keywords": ["benchmark", "cli"],
    "homepage": "https://github.com/asika32764/php-simple-benchmark",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "windwalker/console": "^3.0",
        "windwalker/environment": "^3.0",
        "symfony/stopwatch": "^3.0||^4.0||^5.0"
    },
    "autoload": {
        "psr-4": {
            "SimpleBenchmark\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "bin": [
        "benchmark",
        "sb"
    ]
}
