{
    "name": "asika/simple-benchmark",
    "type": "console",
    "description": "PHP Simple Benchmark framework",
    "keywords": ["benchmark", "cli"],
    "homepage": "https://github.com/asika32764/php-simple-benchmark",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "symfony/console": "^5.0||^6.0||^7.0",
        "symfony/stopwatch": "^5.0||^6.0||^7.0"
    },
    "autoload": {
        "psr-4": {
            "SimpleBenchmark\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.5"
        }
    },
    "bin": [
        "benchmark",
        "sb"
    ],
    "require-dev": {
        "windwalker/utilities": "^4.0"
    }
}
